        "mental_phases": {
            "_0": {
                "title": "PHASE 1: JAGUAR THEORY",
                "desc": "Focus on the objective without getting lost in emotions. Jaguars don't complain; they hunt.",
                "core": "Minimizing emotional reactions when hit by an impulse or difficulty.",
                "intent": "Shifting the focus entirely to 'what should be done now' instead of why things are bad.",
                "strategy": {
                    "_0": "Notice when you start complaining and pause.",
                    "_1": "Convert emotional pain into kinetic energy.",
                    "_2": "Make cold and calculated decisions."
                },
                "practice": {
                    "_0": "Daily: Stop complaining at least once and replace it with an action statement.",
                    "_1": "Daily: When you catch negative self-talk, turn it into a neutral/solution-oriented statement."
                }
            },
            "_1": {
                "title": "PHASE 2: ALGORITHM MINDSET",
                "desc": "Seeing life not as a drama, but as a system of cause and effect.",
                "core": "Viewing results as 'output' and actions as 'input'. When output is bad, fix the input.",
                "intent": "To get rid of 'Why is this happening to me?' mentality and adopt a 'system debugging' mindset.",
                "strategy": {
                    "_0": "Separate incidents from personal narratives.",
                    "_1": "Determine which dynamics are at play in daily events.",
                    "_2": "Do not debug without collecting data first."
                },
                "practice": {
                    "_0": "Daily: Write down one problem and list 3 inputs that led to it.",
                    "_1": "Weekly: Formulate a new algorithm for a recurring issue."
                }
            },
            "_2": {
                "title": "PHASE 3: RED PILL / HARSH TRUTHS",
                "desc": "Confronting the realities you are running away from.",
                "core": "Breaking self-deception.",
                "intent": "Embracing uncomfortable facts rather than comfortable lies.",
                "strategy": {
                    "_0": "Analyze who wants what in affecting situations.",
                    "_1": "Understand that 'hope' is not a strategy.",
                    "_2": "Identify where you are lying to yourself."
                },
                "practice": {
                    "_0": "Daily: Pick 1 event and analyze 'who wanted what'.",
                    "_1": "Daily/Weekly: Write a 3-item log of the painful truths that left a mark on you."
                }
            },
            "_3": {
                "title": "PHASE 4: MINIMUM FRICTION",
                "desc": "Removing obstacles in execution.",
                "core": "Execution depends on reducing friction, not increasing motivation.",
                "intent": "Lowering the barriers to starting tasks.",
                "strategy": {
                    "_0": "Make good habits very easy, bad habits very difficult.",
                    "_1": "Preparation eliminates hesitation.",
                    "_2": "Do the 'first 2 minutes' of the hardest task."
                },
                "practice": {
                    "_0": "Daily: Set 2 micro-routines you will do no matter what.",
                    "_1": "Daily: Even if you don't feel like it, perform the routine in its smallest form."
                }
            },
            "_4": {
                "title": "PHASE 5: ASYNC EXECUTION",
                "desc": "Do not wait for perfect conditions. Execute on the fly.",
                "core": "Perfectionism is procrastination.",
                "intent": "Ship early, iterate fast.",
                "strategy": {
                    "_0": "Start even if you don't feel ready.",
                    "_1": "Identify the critical path and hit it.",
                    "_2": "Embrace failure as data."
                },
                "practice": {
                    "_0": "Daily: Deploy a micro-feature or perform a task instantly.",
                    "_1": "Weekly: Review friction logs and bypass bottlenecks."
                }
            },
            "_5": {
                "title": "PHASE 6: CHAOS TOLERANCE",
                "desc": "Staying calm when the plan collapses.",
                "core": "Embracing uncertainty and chaos as the natural state of existence.",
                "intent": "Enhancing adaptability when unforeseen events destroy the schedule.",
                "strategy": {
                    "_0": "Expect things to go wrong.",
                    "_1": "Focus on your reaction, not the disruption.",
                    "_2": "Pivot instantly without emotional attachment to the plan."
                },
                "practice": {
                    "_0": "Daily: Do 5-10 minutes of micro-action on a task you feel 'not ready' for.",
                    "_1": "Daily: Make a list of 'Today wasn't perfect, but I accomplished these'."
                }
            },
            "_6": {
                "title": "PHASE 7: DOPAMINE DETOX",
                "desc": "Regaining control over cheap dopamine sources.",
                "core": "Understanding that high consumption destroys the ability to produce.",
                "intent": "Resetting the baseline of pleasure by eliminating constant stimulation.",
                "strategy": {
                    "_0": "Cut out passive scrolling and endless feeds.",
                    "_1": "Earn your dopamine through difficult tasks.",
                    "_2": "Embrace boredom."
                },
                "practice": {
                    "_0": "Daily: Leave at least 1 output (code, writing, art).",
                    "_1": "Daily: Lock consumption within a specified time band."
                }
            },
            "_7": {
                "title": "PHASE 8: DARK ARTS",
                "desc": "Going deep into your discipline. Advanced tactics.",
                "core": "Mastery through intense focus and unconventional methods.",
                "intent": "Achieve high-level results by breaking standard norms.",
                "strategy": {
                    "_0": "Exploit system loopholes for efficiency.",
                    "_1": "Read the top tier knowledge.",
                    "_2": "Combine skills aggressively."
                },
                "practice": {
                    "_0": "Daily: Dedicate 30m to advanced theory.",
                    "_1": "Weekly: Run a stress test on your limits."
                }
            }
        },
